#: src/args.c:610
msgid ""
"usage: indent file [-o outfile ] [ options ]\n"
"       indent file1 file2 ... fileN [ options ]\n"
msgstr ""
"употреба: indent файл [-o изхфайл ] [ опции ]\n"
"          indent файл1 файл2 ... файлN [ опции ]\n"

