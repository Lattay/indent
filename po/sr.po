#, c-format
msgid "%s: unknown option \"%s\"\n"
msgstr "%s: непозната опција „%s“\n"

#: src/args.c:811
#, c-format
msgid "option: %s\n"
msgstr "опција: %s\n"

#: src/args.c:827
#, c-format
